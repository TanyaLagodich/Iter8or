{
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js$",
    "excludePattern": "(^|\\/|\\\\)node_modules"
  },
  "opts": {
    "destination": "./docs",
    "recurse": true,
    "readme": "./README.md",
    "template": "node_modules/clean-jsdoc-theme",
    "theme_opts": {
      "title": "Iter8or Documentation"
    }
  }
}
